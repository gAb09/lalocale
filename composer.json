{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gAb09/domaines"
        }
    ],
    "require": {
		"laravel/framework": "4.0.*",
		"baum/baum": "~1.0",
		"gAb09/domaines": "dev-master",
		"barryvdh/laravel-debugbar": "1.*",
		"cartalyst/sentry": "2.0.*",
		"illuminate/database": "4.0.*",
		"ircmaxell/password-compat": "1.0.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php",
			"app/modules",
			"app/lib"
		],
		"psr-4": {
			"lib\\dashboard\\": "app/lib/dashboard",
			"Tresorerie\\Domaines\\": "app/modules/tresorerie/domaines",
			"Tresorerie\\Validations\\": "app/modules/tresorerie/validations",
			"Tresorerie\\Providers\\": "app/modules/tresorerie/providers",
			"Gab\\Domaines\\": "vendor/gab/domaines/src/Gab/Domaines",
			"Gab\\DatesFr\\": "vendor/gab/dates-fr/src/Gab/DatesFr",
			"Gab\\NombresFr\\": "vendor/gab/nombres-fr/src/Gab/NombresFr"
		}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize",
			"php artisan debugbar:publish"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}